{
  "name": "@composio/ao-api-server",
  "version": "0.1.0",
  "description": "Express HTTP API server for agent-orchestrator mobile app backend",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ComposioHQ/agent-orchestrator.git",
    "directory": "packages/api-server"
  },
  "homepage": "https://github.com/ComposioHQ/agent-orchestrator",
  "bugs": {
    "url": "https://github.com/ComposioHQ/agent-orchestrator/issues"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "generate-key": "tsx src/generate-key.ts",
